{
  "name": "Make Twitter Geat Again",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Mute tweets with a single click.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": [
        "contentScript.css"
      ],
      "js": ["contentScript.js"]
    }
  ]
}
